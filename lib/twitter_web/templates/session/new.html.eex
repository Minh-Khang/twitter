<%= form_for @changeset, Routes.session_path(@conn, :create), [as: :session, class: "form-signin"], fn f -> %>
  <h1>Log in</h1>

  <%= label f, :email%>
  <%= text_input f, :email %>
  <%= error_tag f, :email %>

  <%= label f, :password %>
  <%= password_input f, :password %>
  <%= error_tag f, :password %>

  <div><%= submit "Sign in", class: "btn btn-primary" %></div>
<% end %>
